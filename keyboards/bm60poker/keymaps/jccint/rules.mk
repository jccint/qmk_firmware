# Build options
LTO_ENABLE = yes # Link time optimization (disables legacy macros/functions)

# Features
MOUSEKEY_ENABLE = no # Mouse keys
NKRO_ENABLE = yes # USB N-Key Rollover

# RGB
RGB_MATRIX_ENABLE = no # Use RGBLIGHT_ENABLE instead
RGBLIGHT_ENABLE = yes # Enable keyboard RGB lighting (in this case used for lighting layers)

# Debounce algorithm
DEBOUNCE_TYPE = sym_eager_pk
